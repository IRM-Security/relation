{
  "name": "irm/relation",
  "description": "",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "container-interop/container-interop": "^1.2"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.11.2"
  },
  "autoload": {
    "psr-4": {
      "Irm\\Relation\\": "src/Relation/"
    }
  },
  "repositories": [
  ],
  "extra": [],
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t app/ app/index.php",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  },
  "config": {
    "secure-http": false,
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
